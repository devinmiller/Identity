{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.js",
        "js/bootstrap.js.map",
        "js/bootstrap.min.js.map",
        "css/bootstrap.css",
        "css/bootstrap.css.map",
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.min.map",
        "jquery.js",
        "jquery.slim.js",
        "jquery.slim.min.js",
        "jquery.slim.min.map",
        "core.js"
      ]
    },
    {
      "library": "popper.js@1.15.0",
      "destination": "wwwroot/lib/popper/",
      "files": [
        "umd/popper.js",
        "umd/popper.js.map",
        "umd/popper.min.js",
        "umd/popper.min.js.map"
      ]
    },
    {
      "library": "open-iconic@1.1.1",
      "destination": "wwwroot/lib/iconic/",
      "files": [
        "font/css/open-iconic.min.css",
        "font/css/open-iconic.css",
        "font/css/open-iconic-bootstrap.css",
        "font/css/open-iconic-bootstrap.min.css",
        "font/fonts/open-iconic.eot",
        "font/fonts/open-iconic.otf",
        "font/fonts/open-iconic.svg",
        "font/fonts/open-iconic.ttf",
        "font/fonts/open-iconic.woff"
      ]
    }
  ]
}